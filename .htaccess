# Display PHP errors - .htaccess
php_flag display_errors on
php_value error_reporting 6143
###################################

php_value post_max_size 500MB
php_value upload_max_filesize 500MB
#php_value max_execution_time 300

Order Deny,Allow

# local host
Allow from 127.0.0.0/24

# local net
Allow from 192.168.0.0/21

# zbytek sveta
Deny from All


RewriteEngine On
#RewriteBase /
RewriteRule ^$ www/ [L]
RewriteRule (.*) www/$1 [L]

<Files .user.ini>
    Order allow,deny
    Deny from all
</Files>

