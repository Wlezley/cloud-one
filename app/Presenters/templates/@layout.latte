<body>
	{*block styles}
	{/block*}
{if $user->isLoggedIn() && $presenter->name !='Tisk' && $presenter->name !='Cron' && $presenter->name !='Upload'}
	<div class="container-fluid" id="pageWrap">
		<div class="row">
			<div class="col-lg-2 col-md-3 col-sm-4 d-print-none">
				{include 'layout/sidebar.latte'}
			</div>
			<div class="col-lg-10 col-md-9 col-sm-8 contentWrap">
				<div class="content">
					{*include 'layout/alerts.latte'*}
					{include content}
				</div>
			</div>
			{include './layout/footer.latte'}
		</div>
	</div>
{else}
	{include content}
{/if}

	{block scripts}
	<!--script src="https://nette.github.io/resources/js/netteForms.min.js"></script-->
	<script src="https://nette.github.io/resources/js/3/netteForms.min.js"></script>
	<script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
	<script src="{$basePath}/libs/bootstrap/dist/js/bootstrap.min.js"></script>
	{/block}
