	<meta property="og:site_name"	content="ZetServer CLOUD" />
{* TITLE *}
	<title>{ifset title}{include title|striptags} | {/ifset}ZetServer CLOUD</title>
	{*<title>{ifset title}{include title|stripHtml} | {/ifset}ZetServer CLOUD</title>*}
{* CSS *}
	{*<!-- link rel="stylesheet" type="text/css" href="{$basePath}/libs/hover/css/hover-min.css" -->
	<!-- link rel="stylesheet" type="text/css" href="{$basePath}/libs/wow/css/libs/animate.css" -->
	<!-- link rel="stylesheet" type="text/css" href="{$basePath}/libs/sidebar/css/simple-sidebar.css" -->*}
	<link rel="stylesheet" type="text/css" href="{$basePath}/libs/bootstrap/dist/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="{$basePath}/libs/font-awesome/css/all.min.css">
	{*<!-- link rel="stylesheet" type="text/css" href="{$basePath}/libs/font-awesome/css/regular.min.css" -->*}
	<link rel="stylesheet" type="text/css" href="{$basePath}/css/style.css"/>
{* FONTS *}
	<link href="https://fonts.googleapis.com/css?family=Oswald:400,300,700&amp;subset=latin,latin-ext" rel="stylesheet" type="text/css">
	<link rel="shortcut icon" href="/favicon.ico">
{* JAVA SCRIPT *}
	{*block javascript}
	{/block*}
{* HEAD BLOCK *}
	{/block}
	{block head}
	{/block}
