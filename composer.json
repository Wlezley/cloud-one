		"nette/bootstrap": "^3.0",
		"nette/caching": "^3.3",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/finder": "^3.0",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.6",
		"olicek/dropzone-uploader": "dev-master",
		"nesbot/carbon": "^3.8"
	},
	"require-dev": {
		"nette/tester": "^2.0",
		"symfony/thanks": "^1"
